{
  "name": "remote-client",
  "version": "0.0.1",
  "main": "main/index.js",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "start": "concurrently \"ng serve\" \"electron ./main\"",
    "build": "ng build && npm run build-server",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-server": "tsc --lib es6,dom ./main/*.ts",
    "electron": " npm run build-server && electronmon main/index.js",
    "electron-prod": "ng build --base-href ./ && cross-env NODE_ENV=production npm run electron",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.8",
    "@angular/common": "~7.2.8",
    "@angular/compiler": "~7.2.8",
    "@angular/core": "~7.2.8",
    "@angular/forms": "~7.2.8",
    "@angular/platform-browser": "~7.2.8",
    "@angular/platform-browser-dynamic": "~7.2.8",
    "@angular/router": "~7.2.8",
    "@types/ssh2-sftp-client": "^2.4.1",
    "core-js": "^2.5.4",
    "electron-progressbar": "^1.2.0",
    "electron-store": "^3.0.0",
    "md5": "^2.2.1",
    "ngx-electron": "^2.1.1",
    "primeflex": "^1.0.0-rc.1",
    "primeicons": "^1.0.0",
    "primeng": "^7.1.0-rc.1",
    "quill": "^1.3.6",
    "rxjs": "~6.4.0",
    "ssh2-sftp-client": "^2.5.0",
    "tslib": "^1.9.0",
    "winston": "^3.2.1",
    "winston-console-for-electron": "^0.0.6",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.5",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular-guru/electron-builder": "^7.2.14",
    "@angular/cli": "~7.3.5",
    "@angular/compiler-cli": "~7.2.8",
    "@angular/language-service": "~7.2.8",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@types/electron-store": "^1.3.1",
    "@types/jasmine": "~3.3.9",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~11.11.0",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron": "^4.0.6",
    "electronmon": "^0.1.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "remove": "^0.1.5",
    "ts-loader": "^5.3.3",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "3.1.1"
